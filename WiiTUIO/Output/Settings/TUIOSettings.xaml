<Window x:Class="WiiTUIO.Output.TUIOSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TUIO Settings" Height="190" Width="289" Closing="Window_Closing_1" GotFocus="Window_GotFocus_1">
    <Grid>
        <TextBox HorizontalAlignment="Left" Name="tbIP" Height="26" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="171" Margin="100,56,0,0" FontSize="16"/>
        <TextBox HorizontalAlignment="Left" Name="tbPort" Height="26" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="171" Margin="100,86,0,0" FontSize="16"/>
        <Label Content="TUIO Settings" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="22" Foreground="#FF169CCF"/>
        <Label Content="IP Address" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" FontSize="14" Foreground="Black"/>
        <Label Content="Port" HorizontalAlignment="Left" Margin="10,83,0,0" VerticalAlignment="Top" FontSize="14" Foreground="Black"/>
        <Button Content="Done" HorizontalAlignment="Left" Margin="196,129,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
    </Grid>
</Window>
